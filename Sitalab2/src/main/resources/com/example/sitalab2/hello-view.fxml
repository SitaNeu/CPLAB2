<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.sitalab2.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <TableView fx:id="TABLE" prefHeight="200.0" prefWidth="613.0">
     <columns>
       <TableColumn fx:id="sID" prefWidth="75.0" text="StudentID" />
       <TableColumn fx:id="sname" prefWidth="75.0" text="FirstName" />
         <TableColumn fx:id="slast" prefWidth="75.0" text="LastName" />
         <TableColumn fx:id="saddress" prefWidth="75.0" text="Address" />
     </columns>
   </TableView>
   <TextField fx:id="takeID" />
   <TextField fx:id="takeName" />
   <TextField fx:id="takeLast" />
   <TextField fx:id="takeAddress" />
   <Button mnemonicParsing="false" text="insert" />
   <Button mnemonicParsing="false" text="update" />
   <Button mnemonicParsing="false" text="delete" />

    <Label fx:id="welcomeText" />
    <Button onAction="#onHelloButtonClick" text="View Data" />
</VBox>
